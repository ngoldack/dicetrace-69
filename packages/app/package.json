    "eslint-plugin-neverthrow": "^1.1.4",
    "eslint-plugin-svelte": "^2.35.1",
    "pino-pretty": "^10.3.1",
    "postcss": "^8.4.33",
    "postcss-load-config": "^4.0.2",
    "prettier": "^3.2.4",
    "prettier-plugin-svelte": "^3.1.2",
    "prettier-plugin-tailwindcss": "^0.5.11",
    "stripe": "^14.14.0",
    "svelte": "^4.2.9",
    "svelte-check": "^3.6.3",
    "svelte-stripe": "^1.1.3",
    "sveltekit-superforms": "^1.13.4",
