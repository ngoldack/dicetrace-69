{
  "name": "app",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "dev:full": "bun run concurrently --kill-others -n dev,db \"bun dev\" \"bun db\"",
    "dev": "vite dev | pino-pretty",
    "build": "vite build",
    "preview": "vite preview",
    "test": "bun run concurrently -n integration,unit \"bun run test:integration\" \"bun run test:unit\"",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "prettier --check . && eslint .",
    "format": "prettier --write .",
    "test:integration": "playwright test",
    "test:integration:report": "playwright show report",
    "test:unit": "bun test --coverage",
    "migration:gen": "drizzle-kit generate:sqlite",
    "migration:check": "drizzle-kit check:sqlite",
    "migration:push": "drizzle-kit push:sqlite",
    "studio": "bunx drizzle-kit studio",
    "update": "npm-check-updates --interactive --format group",
    "ci": "dagger run node --loader ts-node/esm ./index.mts",
    "db": "turso dev --port 1111"
  },
  "devDependencies": {
    "@iconify/json": "^2.2.175",
