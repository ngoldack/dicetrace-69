    "@typescript-eslint/parser": "^6.19.1",
    "autoprefixer": "^10.4.17",
    "bun-types": "^1.0.25",
    "concurrently": "^8.2.2",
    "dotenv": "^16.4.1",
    "drizzle-kit": "^0.20.13",
    "eslint": "^8.56.0",
    "svelte-headless-table": "^0.18.1",
