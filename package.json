{
  "name": "dicetrace",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "preview": "vite preview",
    "test": "bun run concurrently -n integration,unit \"bun run test:integration\" \"bun run test:unit\"",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "prettier --plugin-search-dir . --check . && eslint .",
    "format": "prettier --plugin-search-dir . --write .",
    "test:integration": "playwright test",
    "test:integration:report": "playwright show report",
    "test:unit": "bun test --coverage",
    "migration:gen": "drizzle-kit generate:sqlite",
    "migration:check": "drizzle-kit check:sqlite",
    "migration:push": "drizzle-kit push:sqlite",
    "studio": "bunx drizzle-kit studio",
    "update": "npm-check-updates --interactive --format group",
    "ci": "dagger run node --loader ts-node/esm ./index.mts"
  },
  "devDependencies": {
    "@iconify/json": "^2.2.140",
    "@playwright/test": "^1.39.0",
    "@sveltejs/kit": "^1.27.5",
    "@typescript-eslint/eslint-plugin": "^6.10.0",
    "@typescript-eslint/parser": "^6.10.0",
    "autoprefixer": "^10.4.16",
    "bun-types": "^1.0.11",
    "concurrently": "^8.2.2",
    "dotenv": "^16.3.1",
    "drizzle-kit": "^0.20.2",
    "eslint": "^8.53.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-svelte": "^2.35.0",
    "flowbite": "^2.1.1",
    "flowbite-svelte": "^0.44.19",
    "flowbite-typography": "^1.0.3",
    "postcss": "^8.4.31",
    "postcss-load-config": "^4.0.1",
    "prettier": "^3.0.3",
    "prettier-plugin-svelte": "^3.1.0",
    "svelte": "^4.2.3",
    "svelte-check": "^3.6.0",
    "sveltekit-superforms": "^1.10.1",
    "tailwindcss": "^3.3.5",
    "tslib": "^2.6.2",
    "typescript": "^5.2.2",
    "unplugin-icons": "^0.17.3",
    "vite": "^4.5.0"
  },
  "type": "module",
  "dependencies": {
    "@auth/core": "^0.18.1",
    "@auth/drizzle-adapter": "^0.3.6",
    "@auth/sveltekit": "^0.3.12",
    "@libsql/client": "^0.3.6",
    "@paralleldrive/cuid2": "^2.2.2",
    "@sveltejs/adapter-vercel": "^3.0.3",
    "@vercel/analytics": "^1.1.1",
    "@vercel/blob": "^0.15.0",
    "@vercel/kv": "^1.0.0",
    "bgg-xml-api-client": "^0.2.0-beta.12",
    "boardgamegeekclient": "^1.9.1",
    "drizzle-orm": "^0.29.0",
    "drizzle-zod": "^0.5.1",
    "flowbite-svelte-blocks": "^0.5.1",
    "npm-check-updates": "^16.14.6",
    "postgres": "^3.4.3",
    "svoast": "^2.3.0"
  }
}
